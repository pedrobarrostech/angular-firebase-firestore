{
  "hosting": {
    "public": "dist/firestarter",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "rewrites": [
    {
      "source" : "**/*.@(css|js)",
      "destination": "/index2.html"
    },
    {
      "source": "**",
      "function": "ssr"
    }
  ]
}
